  .switch.android, .toggle.android { background-color: rgb(182, 182, 182); color: rgb(255, 255, 255); }
  .android.toggle { border: 2px solid rgb(182, 182, 182); }
  .android.switch { overflow: hidden; }
  .android.switch .slide-button { background-color: rgb(39, 159, 202); transform: skew(20deg) translateX(10px); }
  .android.toggle .slide-button { border-radius: 2px; background-color: rgb(132, 132, 132); }
  .android.toggle input:first-of-type:checked ~ .slide-button { background-color: rgb(39, 159, 202); }
  .android.switch input:first-of-type:checked ~ .slide-button { transform: skew(20deg) translateX(-10px); }
  .switch.android p, .toggle.android p { color: rgb(51, 51, 51); }
  .switch.android span, .toggle.android span { color: rgb(255, 255, 255); }
  .android.switch, .switch.android span, .toggle.android span { text-transform: uppercase; }
  .android.switch input { top: 0px; left: 0px; }
  .switch.ios, .toggle.ios { background: -webkit-linear-gradient(top, rgb(207, 207, 207), rgb(239, 239, 239) 50%, rgb(249, 249, 249) 50%, rgb(254, 254, 254)); box-shadow: rgb(182, 182, 182) 0px 2px 2px inset, rgb(182, 182, 182) 3px 0px 3px inset; border: 1px solid rgb(239, 239, 239); border-radius: 3px; color: rgb(127, 127, 127); font: bold 14px sans-serif; text-align: center; text-shadow: none; }
  .ios.toggle .slide-button { border: 1px solid rgb(145, 145, 145); background: -webkit-linear-gradient(top, rgb(205, 205, 205), rgb(251, 251, 251)); border-radius: 3px; box-shadow: rgb(240, 240, 240) 0px 1px 0px inset; }
  .ios.toggle input:first-of-type:checked ~ p span:first-child, .ios.switch input:checked + label { color: rgb(255, 255, 255); text-shadow: rgb(27, 59, 111) 0px -1px 0px; }
  .ios.toggle input:first-of-type:checked ~ .slide-button, .ios.switch .slide-button { color: rgb(255, 255, 255); text-shadow: rgba(0, 0, 0, 0.25) 0px -1px 0px; background-color: rgb(0, 109, 204); background-image: linear-gradient(rgb(0, 136, 204), rgb(0, 68, 204)); background-repeat: repeat-x; border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25); }
  .ios.toggle input:first-of-type:checked ~ .slide-button:hover, .ios.toggle input:first-of-type:checked ~ .slide-button:focus, .ios.toggle input:first-of-type:checked ~ .slide-button:active, .ios.toggle input:first-of-type:checked ~ .slide-button.active, .ios.toggle input:first-of-type:checked ~ .slide-button.disabled, .ios.toggle input:first-of-type:checked ~ .slide-button[disabled], .ios.switch .slide-button:hover, .ios.switch .slide-button:focus, .ios.switch .slide-button:active, .ios.switch .slide-button.active, .ios.switch .slide-button.disabled, .ios.switch .slide-button[disabled] { color: rgb(255, 255, 255); background-color: rgb(0, 68, 204); }
  .ios.toggle input:first-of-type:checked ~ .slide-button:active, .ios.toggle input:first-of-type:checked ~ .slide-button.active, .ios.switch .slide-button:active, .ios.switch .slide-button.active { }
  .ios.toggle p span { color: rgb(127, 127, 127); }
}

