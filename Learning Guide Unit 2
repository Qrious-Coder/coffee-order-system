  .cvaa-adv .icvaa-check::after, .cvaa-size .icvaa-check::after { top: -4px; }
}

.cvaa-arrow { display: none; position: absolute; user-select: none; cursor: pointer; top: 10%; font-size: 20px; }

.cvaa-color .cvaa-btn, .cvaa-bg .cvaa-btn { border: 1px solid rgb(255, 255, 255); padding: 4px; border-radius: 4px; background-clip: content-box; vertical-align: top; }

.cvaa-color .cvaa-btn.White { background-color: transparent; color: rgb(255, 255, 255); }

.cvaa-btn.White { background-color: rgb(255, 255, 255); }

