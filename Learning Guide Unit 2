.cvaa-btn.Yellow { background-color: rgb(255, 255, 0); }

.cvaa-color .cvaa-btn.Green { background-color: transparent; color: rgb(0, 255, 0); }

.cvaa-btn.Green { background-color: rgb(0, 255, 0); }

.cvaa-color .cvaa-btn.Cyan { background-color: transparent; color: rgb(0, 255, 255); }

.cvaa-btn.Cyan { background-color: rgb(0, 255, 255); }

.cvaa-color .cvaa-btn.Blue { background-color: transparent; color: rgb(0, 0, 255); }

.cvaa-btn.Blue { background-color: rgb(0, 0, 255); }

.cvaa-color .cvaa-btn.Magenta { background-color: transparent; color: rgb(255, 0, 255); }

.cvaa-btn.Magenta { background-color: rgb(255, 0, 255); }

.cvaa-color .cvaa-btn.Red { background-color: transparent; color: rgb(255, 0, 0); }

.cvaa-btn.Red { background-color: rgb(255, 0, 0); }

.cvaa-color .cvaa-btn.Black { background-color: transparent; color: rgb(0, 0, 0); }

.cvaa-btn.Black { background-color: rgb(0, 0, 0); }

